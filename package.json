{
  "name": "rocketchat",
  "productName": "Rocket.Chat",
  "description": "Rocket.Chat Native Cross-Platform Desktop Application via Electron.",
  "version": "2.15.0-develop",
  "author": "Rocket.Chat Support <support@rocket.chat>",
  "copyright": "Â© 2018, Rocket.Chat",
  "homepage": "https://rocket.chat",
  "license": "MIT",
  "main": "app/background.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/RocketChat/Rocket.Chat.Electron.git"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "gulp start",
    "build": "gulp build-app",
    "changelog": "conventional-changelog --config .github/changelog.js -i HISTORY.md -s",
    "release": "gulp release --env=production",
    "release-dev": "gulp release --env=development",
    "release-mas-dev": "build --publish never --mac mas-dev --c.mac.provisioningProfile=Development.provisionprofile",
    "lint": "eslint src",
    "pretest": "gulp build-unit-tests --env=test",
    "test-main": "xvfb-maybe electron-mocha --require source-map-support/register app/main.spec.js",
    "test-renderer": "xvfb-maybe electron-mocha --require source-map-support/register --renderer app/renderer.spec.js",
    "test": "npm-run-all test-main test-renderer",
    "precoverage": "gulp build-coverage-tests --env=test",
    "coverage-main": "xvfb-maybe electron-mocha --require source-map-support/register --reporter tasks/coverage-reporter app/main.spec.js",
    "coverage-renderer": "xvfb-maybe electron-mocha --require source-map-support/register --reporter tasks/coverage-reporter --renderer app/renderer.spec.js",
    "coverage": "npm-run-all coverage-main coverage-renderer",
    "postcoverage": "node tasks/coverage-reporter",
    "pree2e": "gulp build-e2e-tests --env=production",
    "e2e": "xvfb-maybe mocha --require source-map-support/register app/e2e.js"
  },
  "dependencies": {
    "@paulcbetts/system-idle-time": "^1.0.4",
    "electron-updater": "^4.0.5",
    "freedesktop-notifications": "^1.4.0",
    "fs-jetpack": "^2.2.0",
    "spellchecker": "^3.5.0",
    "tmp": "^0.0.33"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "@rocket.chat/eslint-config": "^0.1.2",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "conventional-changelog-cli": "^2.0.11",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.0.1",
    "electron": "^3.0.10",
    "electron-builder": "^20.38.2",
    "electron-mocha": "^6.0.4",
    "eslint": "^5.9.0",
    "file-loader": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-batch": "^1.0.5",
    "gulp-file": "^0.4.0",
    "gulp-plumber": "^1.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^5.0.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "nyc": "^13.1.0",
    "run-sequence": "^2.2.1",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2",
    "xvfb-maybe": "^0.2.1"
  },
  "devEngines": {
    "node": ">=8.12.x",
    "npm": ">=4.x",
    "yarn": ">=0.21.3"
  }
}
